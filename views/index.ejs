<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lottery</title>
    <link rel="stylesheet" href="index.css">
</head>
<body>
    <table>
        <tr>
            <th>Date</th>
            <th>Category</th>
            <th>Draw</th>
            <th>Prize</th>
        </tr>
        <% lottery.forEach(function(item) { %>
        <tr> 
            <td><%= item.date %></td>
            <td><%= item.category %></td>
            <td><%= item.draw %></td>
            <td><%= item.prize %></td>
        </tr>
    <% }) %>
    </table> <hr>

    <div class="profile">
        <p>My Profile</p>
        <a href="credits"><button>credits</button></a>
    </div>
